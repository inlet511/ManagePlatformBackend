<h1 class="display-4">Users</h1>
<table class="table table-striped">
	<thead>
		<tr>
			<th>#</th>
			<th>Name</th>
			<th>Password</th>
		</tr>
	</thead>
	<tbody>
		<% users.forEach(function(user){ %>
				<tr>
					<td><%= user.id %></td>
					<td><%= user.name %></td>
					
					<td><%= user.pwd %></td>
				</tr>
		<% }); %>
	</tbody>
</table>