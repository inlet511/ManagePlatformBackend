<h1 class="display-4">Roles</h1>
<table class="table table-striped">
	<thead>
		<tr>
			<th>#</th>
			<th>RoleName</th>
		</tr>
	</thead>
	<tbody>
		<% roles.forEach(function(role){ %>
				<tr>
					<td><%= role.id %></td>
					<td><%= role.name %></td>					

				</tr>
		<% }); %>
	</tbody>
</table>